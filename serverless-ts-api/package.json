{
   "name": "serverless-ts-api",
   "version": "1.0.0",
   "description": "template",
   "main": "index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "nodemon",
      "rebuild:sls": "./sls-build.sh",
      "build:sls": "npx tsc",
      "deploy:sls": "npx serverless deploy"
