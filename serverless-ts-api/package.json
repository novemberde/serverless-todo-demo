   "dependencies": {
      "aws-serverless-express": "^3.3.5",
      "body-parser": "^1.18.3",
      "cors": "^2.8.5",
      "dynamoose": "^1.6.2",
      "express": "^4.17.3",
      "mecab-ya": "^0.1.1",
      "morgan": "^1.9.1",
      "source-map-support": "^0.5.10"
   },
   "devDependencies": {
      "@types/body-parser": "^1.17.0",
      "@types/express": "^4.16.0",
